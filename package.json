{
  "name": "@edx/frontend-app-mf-host",
  "version": "0.1.0",
  "description": "Frontend application template",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openedx/frontend-app-mf-host.git"
  },
  "browserslist": [
    "extends @edx/browserslist-config"
  ],
  "scripts": {
    "build": "openedx webpack",
    "serve": "openedx serve",
    "i18n_extract": "openedx formatjs extract",
    "lint": "openedx eslint .",
    "lint:fix": "openedx eslint --fix .",
    "snapshot": "openedx jest --updateSnapshot",
    "start": "openedx webpack-dev-server --progress",
    "start:with-theme": "paragon install-theme && npm start && npm install",
    "test": "openedx jest --coverage --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "author": "edX",
  "license": "AGPL-3.0",
  "homepage": "https://github.com/openedx/frontend-app-mf-host#readme",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/openedx/frontend-app-mf-host/issues"
  },
  "dependencies": {
    "@edx/brand": "npm:@openedx/brand-openedx@^1.2.2",
    "@edx/frontend-component-footer": "13.0.4",
    "@edx/frontend-component-header": "5.0.2",
    "@module-federation/runtime": "^0.1.12",
    "@openedx/frontend-base": "file:../frontend-base",
    "@openedx/paragon": "^22.0.0",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.9",
    "react-router": "6.22.3",
    "react-router-dom": "6.22.3",
    "redux": "4.2.1"
  },
  "devDependencies": {
    "@edx/browserslist-config": "^1.1.1",
    "@edx/reactifex": "^2.1.1",
    "@module-federation/enhanced": "^0.1.12",
    "glob": "7.2.3",
    "husky": "7.0.4",
    "jest": "29.7.0"
  }
}
